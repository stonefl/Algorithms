public class TestDeque {
    
    public static void main(String[] args) {
        
       Deque<Integer> Dint = new Deque<Integer>();
       
       for(int x = 0; x < 10; x++)
           Dint.addFirst(x);
       
       StdOut.println("Size is: " + Dint.size());
       
       // Print in order
       StdOut.println("Print in numbers in descending order from 99: ");
       for (Integer i: Dint) {
           for (Integer j: Dint) {
               StdOut.println(i + " " + j);
           }
       }
       
       // Print in order
       StdOut.println("Print in order: ");
      
       while (Dint.size() > 0)
       {
           int x = Dint.removeLast();
           StdOut.println(x);
       }  
      
       for(int x = 0; x < 10; x++)
           Dint.addFirst(x);
       
       StdOut.println("Size is: " + Dint.size());
       
         // Print in reverse order
       StdOut.println("Print in reverse order: ");
      
       while (Dint.size() > 0)
       {
           int x = Dint.removeFirst();
           StdOut.println(x);
       }  
       
       for(int x = 0; x < 10; x++)
           Dint.addLast(x);
       
       StdOut.println("Size is: " + Dint.size());
       
         // Print in order
       StdOut.println("Print in order: ");
      
       while (Dint.size() > 0)
       {
           int x = Dint.removeFirst();
           StdOut.println(x);
       }  
        
       for(int x = 0; x < 10; x++)
           Dint.addLast(x);
       
       StdOut.println("Size is: " + Dint.size());
       
         // Print in order
       StdOut.println("Print in reverse order: ");
      
       while (Dint.size() > 0)
       {
           int x = Dint.removeLast();
           StdOut.println(x);
       }  
        
       for(int x = 0; x < 10; x++)
           Dint.addFirst(x);
       
       StdOut.println("Size is: " + Dint.size());
       
         // Print in reverse order
       StdOut.println("Print in reverse order: ");
      
       while (Dint.size() > 0)
       {
           int x = Dint.removeFirst();
           StdOut.println(x);
       }  
    }
}

    
    
    
